{
  "name": "devops-omnibus",
  "displayName": "DevOps Omnibus",
  "description": "An extension that automates developer tasks using natural language commands, integrating with various DevOps and cloud services.",
  "version": "0.1.0",
  "publisher": "your-name",
  "engines": {
    "vscode": "^1.60.0"
  },
  "activationEvents": [
    "onCommand:devops-omnibus.runCommand"
  ],
  "main": "./src/extension.ts",
  "contributes": {
    "commands": [
      {
        "command": "devops-omnibus.runCommand",
        "title": "Run DevOps Command"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "DevOps Omnibus Configuration",
      "properties": {
        "devops-omnibus.apiKey": {
          "type": "string",
          "default": "",
          "description": "API key for accessing external services."
        }
      }
    }
  },
  "scripts": {
    "build": "webpack --mode production",
    "test": "mocha --require ts-node/register test/suite/**/*.test.ts"
  },
  "devDependencies": {
    "typescript": "^4.4.3",
    "webpack": "^5.58.2",
    "webpack-cli": "^4.9.1",
    "mocha": "^9.1.3",
    "ts-node": "^10.4.0",
    "eslint": "^7.32.0",
    "prettier": "^2.4.1"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "dotenv": "^10.0.0"
  }
}